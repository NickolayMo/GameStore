@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@using GameStore.WebUI.ModelViews


@model GamesListViewModel
@{
    ViewBag.Title = "Товары";
}
@foreach (var p in @Model.Games)
{
    @Html.Partial("GameSummaryPartial", p);

}
<pager paging-info="@Model.PagingInfo" uri="@Url" model="@Model" class="btn-group pull-right"></pager>

